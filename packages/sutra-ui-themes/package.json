{
  "name": "@sutra/ui-themes",
  "version": "0.1.0",
  "description": "Theme definitions for Sutra UI Framework - holographic, professional, and command themes",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./holographic": {
      "types": "./dist/holographic.d.ts",
      "import": "./dist/holographic.mjs",
      "require": "./dist/holographic.js"
    },
    "./professional": {
      "types": "./dist/professional.d.ts",
      "import": "./dist/professional.mjs",
      "require": "./dist/professional.js"
    },
    "./command": {
      "types": "./dist/command.d.ts",
      "import": "./dist/command.mjs",
      "require": "./dist/command.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/holographic/index.ts src/professional/index.ts src/command/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts src/holographic/index.ts src/professional/index.ts src/command/index.ts --format esm,cjs --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sutra",
    "ui",
    "theme",
    "design-system",
    "holographic",
    "professional",
    "command"
  ],
  "author": "Sutra AI",
  "license": "MIT",
  "peerDependencies": {
    "@sutra/ui-core": "^0.1.0"
  },
  "devDependencies": {
    "@sutra/ui-core": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nranjan2code/sutra-memory.git",
    "directory": "packages/sutra-ui-themes"
  }
}
